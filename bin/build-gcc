#!/bin/sh

CXX="g++" CXXFLAGS="-std=c++11 -O3 -march=native -Werror -Wall -Wold-style-cast -Woverloaded-virtual -Wsign-promo -Wdouble-promotion -Wconversion -Wsign-conversion -Wmissing-declarations -Wredundant-decls -Wno-unused-parameter -Wno-unused-function -Wno-unused-variable" bin/build-helper || exit 1
